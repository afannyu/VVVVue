{"ast":null,"code":"import { useRouter } from \"vue-router\";\nimport { post } from \"../../utils/request\";\nimport { reactive } from \"@vue/reactivity\";\nimport Toast from \"../../components/Toast.vue\"; // axios默认的，header中的content-type类型\n// axios.defaults.headers.post['Content-Type'] = 'application/json';\n\nexport default {\n  name: \"LoginView\",\n\n  setup() {\n    const data = reactive({\n      username: \"\",\n      password: \"\",\n      showToast: false,\n      toastMsg: ''\n    });\n    const router = useRouter();\n\n    const handleLogin = async () => {\n      try {\n        const result = await post(\"11/api/user/login\", {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.Error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: \"Home\"\n          });\n        } else {\n          data.showToast = true;\n          data.toastMsg = '登录失败';\n        }\n      } catch (e) {\n        data.showToast = true;\n        data.toastMsg = '请求失败';\n      }\n    };\n\n    const handelRegister = () => {\n      router.push({\n        name: \"Register\"\n      });\n    };\n\n    return {\n      handleLogin,\n      handelRegister,\n      data\n    };\n  },\n\n  components: {\n    Toast\n  }\n};","map":{"version":3,"mappings":"AA+BA,SAASA,SAAT,QAA0B,YAA1B;AACA,SAASC,IAAT,QAAqB,qBAArB;AACA,SAASC,QAAT,QAAyB,iBAAzB;AACA,OAAOC,KAAP,MAAkB,4BAAlB,C,CACA;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAIJ,QAAQ,CAAC;MACpBK,QAAQ,EAAE,EADU;MAEpBC,QAAQ,EAAE,EAFU;MAGpBC,SAAS,EAAE,KAHS;MAIpBC,QAAQ,EAAE;IAJU,CAAD,CAArB;IAMA,MAAMC,MAAK,GAAIX,SAAS,EAAxB;;IACA,MAAMY,WAAU,GAAI,YAAY;MAC9B,IAAI;QACF,MAAMC,MAAK,GAAI,MAAMZ,IAAI,CAAC,mBAAD,EAAsB;UAC7CM,QAAQ,EAAED,IAAI,CAACC,QAD8B;UAE7CC,QAAQ,EAAEF,IAAI,CAACE;QAF8B,CAAtB,CAAzB;;QAIA,IAAIK,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;UACvBC,YAAY,CAACC,OAAb,GAAuB,IAAvB;UACAL,MAAM,CAACM,IAAP,CAAY;YAAEb,IAAI,EAAE;UAAR,CAAZ;QACF,CAHA,MAGO;U