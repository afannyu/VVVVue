{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  name: 'LoginView',\n\n  setup() {\n    const router = useRouter();\n\n    const handleLogin = () => {\n      localStorage.isLogin = true; // router.push告诉这个路由重新做一次页面跳转\n      // 访问下一个页面，路由的名字叫做Home的页面\n\n      router.push({\n        name: 'Home'\n      });\n    };\n\n    const handelRegister = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handleLogin,\n      handelRegister\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,EAAxB;;IACA,MAAMK,WAAU,GAAI,MAAI;MACtBC,YAAY,CAACC,OAAb,GAAuB,IAAvB,CADsB,CAEtB;MACA;;MACAH,MAAM,CAACI,IAAP,CAAY;QAACN,IAAI,EAAE;MAAP,CAAZ;IACF,CALA;;IAMA,MAAMO,cAAa,GAAI,MAAI;MACzBL,MAAM,CAACI,IAAP,CAAY;QAACN,IAAI,EAAE;MAAP,CAAZ;IACF,CAFA;;IAGA,OAAO;MACLG,WADK;MACOI;IADP,CAAP;EAGF;;AAhBa,CAAf","names":["useRouter","axios","name","setup","router","handleLogin","localStorage","isLogin","push","handelRegister"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <img src=\"http://www.dell-lee.com/imgs/vue3/user.png\" alt=\"\">\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"text\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入手机号\"\r\n      >\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"password\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入密码\"\r\n      >\r\n    </div>\r\n    <div\r\n    @click=\"handleLogin\"\r\n    class=\"wrapper__login-button\"\r\n    >登录</div>\r\n    <div\r\n    @click=\"handelRegister\"\r\n    class=\"wrapper__login-link\"\r\n    >注册</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'LoginView',\r\n  setup() {\r\n    const router = useRouter()\r\n    const handleLogin = ()=>{\r\n      localStorage.isLogin = true\r\n      // router.push告诉这个路由重新做一次页面跳转\r\n      // 访问下一个页面，路由的名字叫做Home的页面\r\n      router.push({name: 'Home'})\r\n    }\r\n    const handelRegister = ()=>{\r\n      router.push({name: 'Register'})\r\n    }\r\n    return {\r\n      handleLogin,handelRegister\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  img {\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    height: .66rem;\r\n    width: .66rem;\r\n  }\r\n  &__input {\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    height: .48rem;\r\n    background: #F9F9F9;\r\n    border: .01rem solid rgba(0,0,0,0.10);\r\n    border-radius: 6px;\r\n    &__content {\r\n      width: 100%;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      line-height: .48rem;\r\n      color: $content-notice-fontcolor;\r\n      font-size: .16rem;\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button {\r\n    text-align: center;\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n    border-radius: 4px;\r\n    font-size: .16rem;\r\n    background: #0091FF;\r\n    color: #FFFFFF;\r\n  }\r\n  &__login-link {\r\n    font-size: .14rem;\r\n    text-align: center;\r\n    color: $content-notice-fontcolor;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}