{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport { post } from '../../utils/request';\nimport { reactive } from '@vue/reactivity'; // axios默认的，header中的content-type类型\n// axios.defaults.headers.post['Content-Type'] = 'application/json';\n\nexport default {\n  name: 'LoginView',\n\n  setup() {\n    const data = reactive({\n      username: '',\n      password: ''\n    });\n    const router = useRouter();\n\n    const handleLogin = async () => {\n      try {\n        const result = await post('/api/user/login', {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.data?.Error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: 'Home'\n          });\n        } else {\n          alert('登录失败');\n        }\n      } catch (e) {\n        alert('请求失败');\n      }\n    };\n\n    const handelRegister = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handleLogin,\n      handelRegister,\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,SAASA,SAAT,QAA0B,YAA1B;AACA,SAAQC,IAAR,QAAmB,qBAAnB;AACA,SAASC,QAAT,QAAyB,iBAAzB,C,CACA;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAIH,QAAQ,CAAC;MACpBI,QAAQ,EAAE,EADU;MAEpBC,QAAQ,EAAE;IAFU,CAAD,CAArB;IAIA,MAAMC,MAAK,GAAIR,SAAS,EAAxB;;IACA,MAAMS,WAAU,GAAI,YAAS;MAC3B,IAAI;QACF,MAAMC,MAAK,GAAI,MAAMT,IAAI,CAAC,iBAAD,EAAmB;UAC5CK,QAAQ,EAAED,IAAI,CAACC,QAD6B;UAE5CC,QAAQ,EAAEF,IAAI,CAACE;QAF6B,CAAnB,CAAzB;;QAIA,IAAGG,MAAM,EAAEL,IAAR,EAAcM,KAAd,KAAwB,CAA3B,EAA8B;UAC5BC,YAAY,CAACC,OAAb,GAAuB,IAAvB;UACAL,MAAM,CAACM,IAAP,CAAY;YAACX,IAAI,EAAE;UAAP,CAAZ;QACD,CAHD,MAGM;UACJY,KAAK,CAAC,MAAD,CAAL;QACF;MACD,CAXD,CAWC,OAAMC,CAAN,EAAS;QACRD,KAAK,CAAC,MAAD,CAAL;MACF;IACF,CAfA;;IAgBA,MAAME,cAAa,GAAI,MAAI;MACzBT,MAAM,CAACM,IAAP,CAAY;QAACX,IAAI,EAAE;MAAP,CAAZ;IACF,CAFA;;IAGA,OAAO;MACLM,WADK;MACOQ,cADP;MACsBZ;IADtB,CAAP;EAGF;;AA9Ba,CAAf","names":["useRouter","post","reactive","name","setup","data","username","password","router","handleLogin","result","Error","localStorage","isLogin","push","alert","e","handelRegister"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <img src=\"http://www.dell-lee.com/imgs/vue3/user.png\" alt=\"\">\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"text\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入用户名\"\r\n      v-model=\"data.username\"\r\n      >\r\n      <!-- 通过v-model这样的指令，\r\n      将input框的内容与data中的内容双向绑定 -->\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"password\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入密码\"\r\n      v-model=\"data.password\"\r\n      >\r\n    </div>\r\n    <div\r\n    @click=\"handleLogin\"\r\n    class=\"wrapper__login-button\"\r\n    >登录</div>\r\n    <div\r\n    @click=\"handelRegister\"\r\n    class=\"wrapper__login-link\"\r\n    >注册</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport {post} from '../../utils/request'\r\nimport { reactive } from '@vue/reactivity'\r\n// axios默认的，header中的content-type类型\r\n// axios.defaults.headers.post['Content-Type'] = 'application/json';\r\n\r\nexport default {\r\n  name: 'LoginView',\r\n  setup() {\r\n    const data = reactive({\r\n      username: '',\r\n      password: ''\r\n    })\r\n    const router = useRouter()\r\n    const handleLogin = async()=>{\r\n      try {\r\n        const result = await post('/api/user/login',{\r\n        username: data.username,\r\n        password: data.password\r\n        })\r\n        if(result?.data?.Error === 0) {\r\n          localStorage.isLogin = true\r\n          router.push({name: 'Home'})\r\n        }else {\r\n          alert('登录失败')\r\n        }\r\n      }catch(e) {\r\n        alert('请求失败')\r\n      }\r\n    }\r\n    const handelRegister = ()=>{\r\n      router.push({name: 'Register'})\r\n    }\r\n    return {\r\n      handleLogin,handelRegister,data\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  img {\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    height: .66rem;\r\n    width: .66rem;\r\n  }\r\n  &__input {\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    height: .48rem;\r\n    background: #F9F9F9;\r\n    border: .01rem solid rgba(0,0,0,0.10);\r\n    border-radius: 6px;\r\n    &__content {\r\n      width: 100%;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      line-height: .48rem;\r\n      color: $content-notice-fontcolor;\r\n      font-size: .16rem;\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button {\r\n    text-align: center;\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n    border-radius: 4px;\r\n    font-size: .16rem;\r\n    background: #0091FF;\r\n    color: #FFFFFF;\r\n  }\r\n  &__login-link {\r\n    font-size: .14rem;\r\n    text-align: center;\r\n    color: $content-notice-fontcolor;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}