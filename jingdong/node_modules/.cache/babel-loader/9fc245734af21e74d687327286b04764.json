{"ast":null,"code":"import { computed } from '@vue/runtime-core';\nimport { useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'CartView',\n\n  setup() {\n    // 获取store中的数据\n    const store = useStore();\n    const route = useRoute();\n    const cartList = store.state.cartList;\n    const shopId = route.params.id; // 计算购物车商品总数量，\n\n    const all = computed(() => {\n      const productList = cartList[shopId];\n      let count = 0;\n\n      if (productList) {\n        for (let i in productList) {\n          const product = productList[i];\n          count += product.count;\n        }\n      }\n\n      return count;\n    }); // 计算购物车商品总价格，\n\n    const price = computed(() => {\n      const productList = cartList[shopId];\n      let count = 0;\n\n      if (productList) {\n        for (let i in productList) {\n          const product = productList[i];\n          count += product.count * product.price;\n        }\n      }\n\n      return count.toFixed(2);\n    }); // 拿出购物车的内容并显示\n\n    const productList = computed(() => {\n      const productList = cartList[shopId] || [];\n      return productList;\n    });\n    return {\n      all,\n      price,\n      productList\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6CA,SAASA,QAAT,QAAyB,mBAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,KAAK,GAAG;IACN;IACA,MAAMC,KAAI,GAAIH,QAAQ,EAAtB;IACA,MAAMI,KAAI,GAAIL,QAAQ,EAAtB;IACA,MAAMM,QAAO,GAAIF,KAAK,CAACG,KAAN,CAAYD,QAA7B;IACA,MAAME,MAAK,GAAIH,KAAK,CAACI,MAAN,CAAaC,EAA5B,CALM,CAMN;;IACA,MAAMC,GAAE,GAAIZ,QAAQ,CAAC,MAAI;MACvB,MAAMa,WAAU,GAAIN,QAAQ,CAACE,MAAD,CAA5B;MACA,IAAIK,KAAI,GAAI,CAAZ;;MACA,IAAGD,WAAH,EAAgB;QACd,KAAI,IAAIE,CAAR,IAAaF,WAAb,EAA0B;UACxB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAD,CAA3B;UACAD,KAAI,IAAKE,OAAO,CAACF,KAAjB;QACF;MACF;;MACA,OAAOA,KAAP;IACD,CAVmB,CAApB,CAPM,CAkBN;;IACA,MAAMG,KAAI,GAAIjB,QAAQ,CAAC,MAAI;MACzB,MAAMa,WAAU,GAAIN,QAAQ,CAACE,MAAD,CAA5B;MACA,IAAIK,KAAI,GAAI,CAAZ;;MACA,IAAGD,WAAH,EAAgB;QACd,KAAI,IAAIE,CAAR,IAAaF,WAAb,EAA0B;UACxB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAD,CAA3B;UACAD,KAAI,IAAME,OAAO,CAACF,KAAR,GAAgBE,OAAO,CAACC,KAAlC;QACF;MACF;;MACA,OAAOH,KAAK,CAACI,OAAN,CAAc,CAAd,CAAP;IACD,CAVqB,CAAtB,CAnBM,CA8BN;;IACA,MAAML,WAAU,GAAIb,QAAQ,CAAC,MAAI;MAC/B,MAAMa,WAAU,GAAIN,QAAQ,CAACE,MAAD,CAAR,IAAoB,EAAxC;MACA,OAAOI,WAAP;IACD,CAH2B,CAA5B;IAKA,OAAO;MACLD,GADK;MACAK,KADA;MACOJ;IADP,CAAP;EAGF;;AAzCa,CAAf","names":["computed","useRoute","useStore","name","setup","store","route","cartList","state","shopId","params","id","all","productList","count","i","product","price","toFixed"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <div class=\"product\">\r\n      <div\r\n      v-for=\"item in productList\"\r\n      :key=\"item._id\"\r\n      class=\"product__item\"\r\n      >\r\n        <img :src=\"item.imgUrl\" alt=\"\">\r\n        <div class=\"product__item__text\">\r\n          <h4>{{item.name}}</h4>\r\n          <div class=\"price\">\r\n            <span class=\"yen\">￥{{item.price}}</span>\r\n            <span class=\"origin\">￥{{item.oldPrice}}</span>\r\n          </div>\r\n          <div class=\"number\">\r\n            <div\r\n            class=\"number__minus\"\r\n            @click=\"()=>{changeCartItem(shopId, item._id, item, -1)}\"\r\n            >-</div>\r\n            <div class=\"number__num\">\r\n              {{ item.count || 0 }}\r\n            </div>\r\n            <div\r\n            class=\"number__plus\"\r\n            @click=\"()=>{changeCartItem(shopId, item._id, item, 1)}\"\r\n            >+</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"checkk\">\r\n      <div class=\"icon\">\r\n      <img src=\"http://www.dell-lee.com/imgs/vue3/basket.png\" alt=\"\">\r\n      <div class=\"icon__tag\">{{all}}</div>\r\n      </div>\r\n      <div class=\"check\">\r\n        总计：<span>￥{{price}}</span>\r\n      </div>\r\n      <div class=\"car\">去结算</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from '@vue/runtime-core'\r\nimport { useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\nexport default {\r\n  name: 'CartView',\r\n  setup() {\r\n    // 获取store中的数据\r\n    const store = useStore()\r\n    const route = useRoute()\r\n    const cartList = store.state.cartList\r\n    const shopId = route.params.id\r\n    // 计算购物车商品总数量，\r\n    const all = computed(()=>{\r\n      const productList = cartList[shopId]\r\n      let count = 0\r\n      if(productList) {\r\n        for(let i in productList) {\r\n          const product = productList[i]\r\n          count += product.count\r\n        }\r\n      }\r\n      return count\r\n    })\r\n    // 计算购物车商品总价格，\r\n    const price = computed(()=>{\r\n      const productList = cartList[shopId]\r\n      let count = 0\r\n      if(productList) {\r\n        for(let i in productList) {\r\n          const product = productList[i]\r\n          count += (product.count * product.price)\r\n        }\r\n      }\r\n      return count.toFixed(2)\r\n    })\r\n    // 拿出购物车的内容并显示\r\n    const productList = computed(()=>{\r\n      const productList = cartList[shopId] || []\r\n      return productList\r\n    })\r\n\r\n    return {\r\n      all, price, productList\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n\r\n.cart {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n\r\n.checkk {\r\n  display: flex;\r\n  height: .49rem;\r\n  box-shadow: 0 -1px 1px 0 #F1F1F1;\r\n}\r\n\r\n.product {\r\n  flex: 1;\r\n  padding: 0 .18rem 0 .16rem;\r\n  overflow-y: scroll;\r\n  background: $bgColor;\r\n  &__item {\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    border-bottom: .01rem solid $content-bgcolor;\r\n    color: $content-fontcolor;\r\n    img {\r\n      margin-right: .16rem;\r\n      height: .68rem;\r\n      width: .68rem;\r\n    }\r\n    &__text {\r\n      position: relative;\r\n      flex: 1;\r\n      overflow: hidden;\r\n      h4 {\r\n        margin: 0;\r\n        font-size: .14rem;\r\n        line-height: .2rem;\r\n        color: $content-fontcolor;\r\n        @include ellipsis\r\n      }\r\n      .price {\r\n        .yen {\r\n          margin-right: .02rem;\r\n          line-height: .2rem;\r\n          font-size: .12rem;\r\n          color: $highlight-fontColor;\r\n        }\r\n        .origin {\r\n          display: inline-block;\r\n          font-size: .12rem;\r\n          transform: scale(.83);\r\n          transform-origin: center;\r\n          text-decoration: line-through;\r\n          color: $light-fontColor;\r\n        }\r\n      }\r\n      .number {\r\n        position: absolute;\r\n        display: flex;\r\n        line-height: .2rem;\r\n        right: 0;\r\n        bottom: 0;\r\n        &__num {\r\n          margin: 0 .1rem;\r\n          font-size: .14rem;\r\n        }\r\n        &__minus,&__plus {\r\n          line-height: .16rem;\r\n          box-sizing: border-box;\r\n          height: .2rem;\r\n          width: .2rem;\r\n          font-size: .18rem;\r\n          text-align: center;\r\n          border-radius: 50%;\r\n        }\r\n        &__minus {\r\n          border: .01rem solid $mid-fontColor;\r\n          color: $mid-fontColor;\r\n        }\r\n        &__plus {\r\n          background: $btn-bgColor;\r\n          color: $bgColor;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.icon {\r\n  position: relative;\r\n  width: .84rem;\r\n  img {\r\n    display: block;\r\n    height: .26rem;\r\n    width: .28rem;\r\n    margin: .12rem auto;\r\n  }\r\n  &__tag {\r\n    position: absolute;\r\n    top: .03rem;\r\n    left: .46rem;\r\n    height: .2rem;\r\n    padding: 0 .04rem;\r\n    min-width: .2rem;\r\n    line-height: .2rem;\r\n    text-align: center;\r\n    transform: scale(50%);\r\n    transform-origin: left center;\r\n    border-radius: .1rem;\r\n    background-color: $highlight-fontColor;\r\n    color: $bgColor;\r\n  }\r\n}\r\n\r\n.check {\r\n  flex: 1;\r\n  font-size: .12rem;\r\n  margin: .14rem 0;\r\n  color: $content-fontcolor;\r\n  span {\r\n    display: inline-block;\r\n    font-size: .18rem;\r\n    color: $highlight-fontColor;\r\n  }\r\n}\r\n\r\n.car {\r\n  line-height: .49rem;\r\n  width: .98rem;\r\n  background: #4FB0F9;\r\n  color: $bgColor;\r\n  text-align: center;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}