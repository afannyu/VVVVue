r\"\r\n    class=\"wrapper__login-link\"\r\n    >注册</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'LoginView',\r\n  setup() {\r\n    const router = useRouter()\r\n    const handleLogin = ()=>{\r\n      axios.post('/api/user/login',{\r\n        username: 'fanyu',\r\n        password: '12345'\r\n      }).then(()=>{\r\n        alert('成功')\r\n      }).catch(()=>{\r\n        alert('失败')\r\n      })\r\n    }\r\n    const handelRegister = ()=>{\r\n      router.push({name: 'Register'})\r\n    }\r\n    return {\r\n      handleLogin,handelRegister\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  img {\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    height: .66rem;\r\n    width: .66rem;\r\n  }\r\n  &__input {\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    height: .48rem;\r\n    background: #F9F9F9;\r\n    border: .01rem solid rgba(0,0,0,0.10);\r\n    border-radius: 6px;\r\n    &__content {\r\n      width: 100%;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      line-height: .48rem;\r\n      color: $content-notice-fontcolor;\r\n      font-size: .16rem;\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button {\r\n    text-align: center;\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n    border-radius: 4px;\r\n    font-size: .16rem;\r\n    background: #0091FF;\r\n    color: #FFFFFF;\r\n  }\r\n  &__login-link {\r\n    font-size: .14rem;\r\n    text-align: center;\r\n    color: $content-notice-fontcolor;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}