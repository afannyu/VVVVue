{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport { reactive } from '@vue/reactivity'; // axios默认的，header中的content-type类型\n\naxios.defaults.headers.post['Content-Type'] = 'application/json';\nexport default {\n  name: 'LoginView',\n\n  setup() {\n    const data = reactive({\n      username: '',\n      password: ''\n    });\n    const router = useRouter();\n\n    const handleLogin = async () => {\n      const result = await axios.post('https://www.fastmock.site/mock/4a01d3c1e70b246641548e2cc9e32657/jingdong/api/user/login', {\n        username: data.username,\n        password: data.password\n      });\n\n      if (result?.data?.error === 0) {\n        localStorage.isLogin = true;\n        router.push({\n          name: 'Home'\n        });\n      } else {\n        alert('登录失败');\n      }\n    };\n\n    const handelRegister = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handleLogin,\n      handelRegister,\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,iBAAzB,C,CACA;;AACAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,IAAvB,CAA4B,cAA5B,IAA8C,kBAA9C;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAIN,QAAQ,CAAC;MACpBO,QAAQ,EAAE,EADU;MAEpBC,QAAQ,EAAE;IAFU,CAAD,CAArB;IAIA,MAAMC,MAAK,GAAIX,SAAS,EAAxB;;IACA,MAAMY,WAAU,GAAI,YAAS;MAC3B,MAAMC,MAAK,GAAI,MAAMZ,KAAK,CAACI,IAAN,CAAW,yFAAX,EAAqG;QACxHI,QAAQ,EAAED,IAAI,CAACC,QADyG;QAExHC,QAAQ,EAAEF,IAAI,CAACE;MAFyG,CAArG,CAArB;;MAIA,IAAGG,MAAM,EAAEL,IAAR,EAAcM,KAAd,KAAwB,CAA3B,EAA8B;QAC5BC,YAAY,CAACC,OAAb,GAAuB,IAAvB;QACAL,MAAM,CAACM,IAAP,CAAY;UAACX,IAAI,EAAE;QAAP,CAAZ;MACD,CAHD,MAGM;QACJY,KAAK,CAAC,MAAD,CAAL;MACF;IACF,CAXA;;IAYA,MAAMC,cAAa,GAAI,MAAI;MACzBR,MAAM,CAACM,IAAP,CAAY;QAACX,IAAI,EAAE;MAAP,CAAZ;IACF,CAFA;;IAGA,OAAO;MACLM,WADK;MACOO,cADP;MACsBX;IADtB,CAAP;EAGF;;AA1Ba,CAAf","names":["useRouter","axios","reactive","defaults","headers","post","name","setup","data","username","password","router","handleLogin","result","error","localStorage","isLogin","push","alert","handelRegister"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <img src=\"http://www.dell-lee.com/imgs/vue3/user.png\" alt=\"\">\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"text\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入用户名\"\r\n      v-model=\"data.username\"\r\n      >\r\n      <!-- 通过v-model这样的指令，\r\n      将input框的内容与data中的内容双向绑定 -->\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input type=\"password\"\r\n      class=\"wrapper__input__content\"\r\n      placeholder=\"请输入密码\"\r\n      v-model=\"data.password\"\r\n      >\r\n    </div>\r\n    <div\r\n    @click=\"handleLogin\"\r\n    class=\"wrapper__login-button\"\r\n    >登录</div>\r\n    <div\r\n    @click=\"handelRegister\"\r\n    class=\"wrapper__login-link\"\r\n    >注册</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport axios from 'axios'\r\nimport { reactive } from '@vue/reactivity'\r\n// axios默认的，header中的content-type类型\r\naxios.defaults.headers.post['Content-Type'] = 'application/json';\r\n\r\nexport default {\r\n  name: 'LoginView',\r\n  setup() {\r\n    const data = reactive({\r\n      username: '',\r\n      password: ''\r\n    })\r\n    const router = useRouter()\r\n    const handleLogin = async()=>{\r\n      const result = await axios.post('https://www.fastmock.site/mock/4a01d3c1e70b246641548e2cc9e32657/jingdong/api/user/login',{\r\n        username: data.username,\r\n        password: data.password\r\n      })\r\n      if(result?.data?.error === 0) {\r\n        localStorage.isLogin = true\r\n        router.push({name: 'Home'})\r\n      }else {\r\n        alert('登录失败')\r\n      }\r\n    }\r\n    const handelRegister = ()=>{\r\n      router.push({name: 'Register'})\r\n    }\r\n    return {\r\n      handleLogin,handelRegister,data\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  img {\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    height: .66rem;\r\n    width: .66rem;\r\n  }\r\n  &__input {\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    height: .48rem;\r\n    background: #F9F9F9;\r\n    border: .01rem solid rgba(0,0,0,0.10);\r\n    border-radius: 6px;\r\n    &__content {\r\n      width: 100%;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      line-height: .48rem;\r\n      color: $content-notice-fontcolor;\r\n      font-size: .16rem;\r\n      &::placeholder {\r\n        color: $content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button {\r\n    text-align: center;\r\n    margin: .32rem .4rem .16rem .4rem;\r\n    line-height: .48rem;\r\n    box-shadow: 0 4px 8px 0 rgba(0,145,255,0.32);\r\n    border-radius: 4px;\r\n    font-size: .16rem;\r\n    background: #0091FF;\r\n    color: #FFFFFF;\r\n  }\r\n  &__login-link {\r\n    font-size: .14rem;\r\n    text-align: center;\r\n    color: $content-notice-fontcolor;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}