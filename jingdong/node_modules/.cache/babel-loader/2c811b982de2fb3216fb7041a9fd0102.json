=\"product\">\r\n      <h4>{{shopName}}</h4>\r\n      <div\r\n      v-for=\"item in productList\"\r\n      :key=\"item._id\"\r\n      class=\"product__item\"\r\n      >\r\n        <img :src=\"item.imgUrl\" alt=\"\">\r\n        <div class=\"product__item__text\">\r\n          <h4>{{item.name}}</h4>\r\n          <div class=\"price\">\r\n            <span class=\"yen\">￥{{item.price}} × {{item.count}}</span>\r\n            <span class=\"origin\">{{(item.price * item.count).toFixed(2)}}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"order\">\r\n    <span class=\"order__price\">实付金额：￥{{}}</span>\r\n    <span class=\"order__order\">提交订单</span>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { commonCartEffect  } from '@/effects/cartEffects';\r\nimport { useRoute } from 'vue-router';\r\nimport { useRouterEffect } from '../../effects/routerEffects'\r\n\r\nexport default {\r\n  name: 'OrderView',\r\n  setup() {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { productList, cartList, shopName } = commonCartEffect(shopId)\r\n    const { handleClickBack } = useRouterEffect()\r\n\r\n    return {\r\n      productList, cartList, shopName, handleClickBack\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  top: 0;\r\n  background: #eee;\r\n}\r\n.top {\r\n  padding: .21rem .18rem;\r\n  box-sizing: border-box;\r\n  height: 1.59rem;\r\n  background-image: linear-gradient(0deg, rgba(0,145,255,0.00) 4%, #0091FF 50%);\r\n  &__order {\r\n    position: relative;\r\n    line-height: .32rem;\r\n    color: $bgColor;\r\n    font-size: .16rem;\r\n    text-align: center;\r\n    &__icon {\r\n      position: absolute;\r\n      top: 0;\r\n      left: 0;\r\n      font-size: .32rem;\r\n    }\r\n  }\r\n  &__receiver {\r\n    position: relative;\r\n    margin: .14rem 0 .16rem 0;\r\n    border-radius: 4px;\r\n    background: $bgColor;\r\n    height: 1.11rem;\r\n    box-sizing: border-box;\r\n    padding: .16rem;\r\n    &__title {\r\n      margin: 0;\r\n      line-height: .22rem;\r\n      font-size: .16rem;\r\n    }\r\n    &__address {\r\n      margin: .1rem 0 .06rem 0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n    }\r\n    &__icon {\r\n      position: absolute;\r\n      right: .16rem;\r\n      line-height: .22rem;\r\n      font-size: .22rem;\r\n      top: .46rem;\r\n      transform: rotate(180deg);\r\n    }\r\n    &__info {\r\n      color: $mid-fontColor;\r\n      line-height: .17rem;\r\n      font-size: .12rem;\r\n    }\r\n  }\r\n}\r\n\r\n.products {\r\n  padding: .18rem;\r\n  margin-top: .1rem;\r\n}\r\n\r\n.product {\r\n  box-sizing: border-box;\r\n  background: $bgColor;\r\n  border-radius: 4px;\r\n  padding: .16rem;\r\n  h4 {\r\n    margin: 0;\r\n    line-height: .22rem;\r\n    font-size: .14rem;\r\n  }\r\n  &__item {\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    color: $content-fontcolor;\r\n    img {\r\n      margin-right: .16rem;\r\n      height: .46rem;\r\n      width: .46rem;\r\n    }\r\n    &__text {\r\n      position: relative;\r\n      flex: 1;\r\n      overflow: hidden;\r\n      h4 {\r\n        margin: 0 0 .06rem 0;\r\n        font-size: .14rem;\r\n        line-height: .2rem;\r\n        color: $content-fontcolor;\r\n        @include ellipsis\r\n      }\r\n      .price {\r\n        position: relative;\r\n        line-height: .2rem;\r\n        font-size: .12rem;\r\n        .yen {\r\n          margin-right: .02rem;\r\n          color: $highlight-fontColor;\r\n        }\r\n        .origin {\r\n          position: absolute;\r\n          right: 0;\r\n          top: 0;\r\n          display: inline-block;\r\n          color: $content-fontcolor;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.order {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  line-height: .49rem;\r\n  height: .49rem;\r\n  background: $bgColor;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}