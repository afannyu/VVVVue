{"ast":null,"code":"import { useRouter } from \"vue-router\";\nimport { reactive } from \"@vue/reactivity\";\nimport Toast from \"../../components/Toast.vue\";\nimport { post } from \"../../utils/request\";\n\nconst useToastEffect = () => {\n  const toastData = reactive({\n    showToast: false,\n    toastMsg: ''\n  });\n\n  const showToast = msg => {\n    toastData.showToast = true;\n    toastData.toastMsg = msg;\n    setTimeout(() => {\n      toastData.showToast = false;\n      toastData.toastMsg = '';\n    }, 1500);\n  };\n\n  return {\n    toastData,\n    showToast\n  };\n};\n\nexport default {\n  name: \"LoginView\",\n\n  setup() {\n    const data = reactive({\n      username: \"\",\n      password: \"\"\n    });\n    const router = useRouter();\n    const {\n      toastData,\n      showToast\n    } = useToastEffect();\n\n    const handleLogin = async () => {\n      try {\n        const result = await post(\"11/api/user/login\", {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.Error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: \"Home\"\n          });\n        } else {\n          showToast('登录失败');\n        }\n      } catch (e) {\n        showToast('请求失败');\n      }\n    };\n\n    const handelRegister = () => {\n      router.push({\n        name: \"Register\"\n      });\n    };\n\n    return {\n      handleLogin,\n      handelRegister,\n      data,\n      toastData,\n      showToast\n    };\n  },\n\n  components: {\n    Toast\n  }\n};","map":{"version":3,"mappings":"AA+BA,SAASA,SAAT,QAA0B,YAA1B;AACA,SAASC,QAAT,QAAyB,iBAAzB;AACA,OAAOC,KAAP,MAAkB,4BAAlB;AACA,SAASC,IAAT,QAAqB,qBAArB;;AAEA,MAAMC,cAAa,GAAI,MAAI;EACzB,MAAMC,SAAQ,GAAIJ,QAAQ,CAAC;IACzBK,SAAS,EAAE,KADc;IAEzBC,QAAQ,EAAE;EAFe,CAAD,CAA1B;;EAIA,MAAMD,SAAQ,GAAKE,GAAD,IAAO;IACvBH,SAAS,CAACC,SAAV,GAAsB,IAAtB;IACAD,SAAS,CAACE,QAAV,GAAqBC,GAArB;IACAC,UAAU,CAAC,MAAI;MACbJ,SAAS,CAACC,SAAV,GAAsB,KAAtB;MACAD,SAA