{"ast":null,"code":"import { get } from \"@/utils/request\";\nimport { ref } from \"@vue/reactivity\";\n\nconst useNearbyListEffect = () => {\n  const nearbyList = ref([]);\n\n  const getNearbyList = async () => {\n    try {\n      const result = await get(\"/api/shop/hot-list\");\n\n      if (result?.errno === 0 && result?.data?.length) {\n        nearbyList.value = result.data;\n      }\n    } catch (e) {\n      alert(\"十年\");\n    }\n  };\n\n  return {\n    getNearbyList,\n    nearbyList\n  };\n};\n\nexport default {\n  name: \"NearbyView\",\n\n  setup() {\n    const {\n      getNearbyList,\n      nearbyList\n    } = useNearbyListEffect();\n    getNearbyList();\n    return {\n      nearbyList,\n      getNearbyList\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,GAAT,QAAoB,iBAApB;;AAEA,MAAMC,mBAAkB,GAAI,MAAI;EAC9B,MAAMC,UAAS,GAAIF,GAAG,CAAC,EAAD,CAAtB;;EACA,MAAMG,aAAY,GAAI,YAAY;IAChC,IAAI;MACF,MAAMC,MAAK,GAAI,MAAML,GAAG,CAAC,oBAAD,CAAxB;;MACA,IAAGK,MAAM,EAAEC,KAAR,KAAkB,CAAlB,IAAuBD,MAAM,EAAEE,IAAR,EAAcC,MAAxC,EAAgD;QAC9CL,UAAU,CAACM,KAAX,GAAmBJ,MAAM,CAACE,IAA1B;MACF;IACF,CALA,CAKE,OAAOG,CAAP,EAAU;MACVC,KAAK,CAAC,IAAD,CAAL;IACF;EACD,CATD;;EAUA,OAAO;IACLP,aADK;IACSD;EADT,CAAP;AAGF,CAfA;;AAgBA,eAAe;EACbS,IAAI,EAAE,YADO;;EAEbC,KAAK,GAAG;IACN,MAAM;MAAET,aAAF;MAAgBD;IAAhB,IAA+BD,mBAAmB,EAAxD;IACAE,aAAa;IACb,OAAO;MACLD,UADK;MACMC;IADN,CAAP;EAGD;;AARY,CAAf","names":["get","ref","useNearbyListEffect","nearbyList","getNearbyList","result","errno","data","length","value","e","alert","name","setup"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\home\\Nearby.vue"],"sourcesContent":["<template>\r\n  <div class=\"nearby\">\r\n    <h3>附近店铺</h3>\r\n    <div v-for=\"item in nearbyList\" :key=\"item._id\" class=\"nearby__item\">\r\n      <img :src=\"item.imgUrl\" alt=\"\" />\r\n      <div class=\"nearby__content\">\r\n        <div class=\"nearby__content__title\">{{ item.name }}</div>\r\n        <div class=\"nearb