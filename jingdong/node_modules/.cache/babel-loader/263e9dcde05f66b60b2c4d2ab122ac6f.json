{"ast":null,"code":"import ShopInfo from \"@/components/ShopInfo\";\nimport Cart from '../shop/Cart.vue';\nimport { useRoute } from 'vue-router';\nimport { reactive, toRefs } from '@vue/reactivity';\nimport { get } from '@/utils/request';\nimport Content from './Content.vue';\nimport { useRouterEffect } from \"@/effects/routerEffects\";\n\nconst useGetItemEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n\n  const getItem = async () => {\n    const result = await get(`/api/shop/${route.params.id}`);\n\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data[`${route.params.id - 1}`];\n    }\n\n    console.log(result);\n  };\n\n  const {\n    item\n  } = toRefs(data);\n  return {\n    getItem,\n    item\n  };\n};\n\nexport default {\n  name: \"ShopView\",\n  components: {\n    ShopInfo,\n    Content,\n    Cart\n  },\n\n  setup() {\n    const {\n      getItem,\n      item\n    } = useGetItemEffect();\n    const {\n      handleClickBack\n    } = useRouterEffect();\n    getItem();\n    return {\n      item,\n      getItem,\n      handleClickBack\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0BA,OAAOA,QAAP,MAAqB,uBAArB;AACA,OAAOC,IAAP,MAAiB,kBAAjB;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAASC,QAAT,EAAmBC,MAAnB,QAAiC,iBAAjC;AACA,SAASC,GAAT,QAAoB,iBAApB;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,SAASC,eAAT,QAAgC,yBAAhC;;AAEA,MAAMC,gBAAe,GAAI,MAAI;EAC3B,MAAMC,KAAI,GAAIP,QAAQ,EAAtB;EACA,MAAMQ,IAAG,GAAIP,QAAQ,CAAC;IAACQ,IAAI,EAAE;EAAP,CAAD,CAArB;;EACA,MAAMC,OAAM,GAAI,YAAU;IACxB,MAAMC,MAAK,GAAI,MAAMR,GAAG,CAAE,aAAYI,KAAK,CAACK,MAAN,CAAaC,EAAG,EAA9B,CAAxB;;IACA,IAAGF,MAAM,EAAEG,KAAR,KAAkB,CAAlB,IAAuBH,MAAM,EAAEH,IAAlC,EAAuC;MACrCA,IAAI,CAACC,IAAL,GAAYE,MAAM,CAACH,IAAP,CAAa,GAAED,KAAK,CAACK,MAAN,CAAaC,EAAb,GAAkB,CAAE,EAAnC,CAAZ;IACF;;IACAE,OAAO,CAACC,GAAR,CAAYL,MAAZ;EACF,CANA;;EAOA,MAAM;IAAEF;EAAF,IAAWP,MAAM,CAACM,IAAD,CAAvB;EACA,OAAO;IACLE,OADK;IACGD;EADH,CAAP;AAGF,CAdA;;AAgBA,eAAe;EACbQ,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IAAEpB,QAAF;IAAYM,OAAZ;IAAqBL;EAArB,CAFC;;EAGboB,KAAK,GAAG;IACN,MAAM;MAAET,OAAF;MAAUD;IAAV,IAAmBH,gBAAgB,EAAzC;IACA,MAAM;MAAEc;IAAF,IAAsBf,eAAe,EAA3C;IAEAK,OAAO;IACP,OAAO;MACLD,IADK;MACCC,OADD;MACUU;IADV,CAAP;EAGD;;AAXY,CAAf","names":["ShopInfo","Cart","useRoute","reactive","toRefs","get","Content","useRouterEffect","useGetItemEffect","route","data","item","getItem","result","params","id","errno","console","log","name","components","setup","handleClickBack"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\shop\\Shop.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"search\">\r\n      <div\r\n      @click=\"handleClickBack\"\r\n      class=\"search__icon iconfont\"\r\n      >\r\n      &#xe710;\r\n      </div>\r\n      <div class=\"search__content\">\r\n        <span class=\"iconfont\">&#xe611;</span>\r\n        <input type=\"text\" placeholder=\"请输入商品名称搜索\" />\r\n      </div>\r\n    </div>\r\n    <ShopInfo\r\n    :item=\"item\"\r\n    :hideBorder=\"true\"\r\n    v-show=\"item.imgUrl\"\r\n    />\r\n  </div>\r\n  <Content :shopName=\"item.name\"/>\r\n  <Cart />\r\n</template>\r\n\r\n\r\n<script>\r\nimport ShopInfo from \"@/components/ShopInfo\";\r\nimport Cart from '../shop/Cart.vue'\r\nimport { useRoute } from 'vue-router';\r\nimport { reactive, toRefs } from '@vue/reactivity';\r\nimport { get } from '@/utils/request';\r\nimport Content from './Content.vue';\r\nimport { useRouterEffect } from \"@/effects/routerEffects\";\r\n\r\nconst useGetItemEffect = ()=>{\r\n  const route = useRoute()\r\n  const data = reactive({item: {}})\r\n  const getItem = async ()=>{\r\n    const result = await get(`/api/shop/${route.params.id}`)\r\n    if(result?.errno === 0 && result?.data){\r\n      data.item = result.data[`${route.params.id - 1}`]\r\n    }\r\n    console.log(result)\r\n  }\r\n  const { item } = toRefs(data)\r\n  return {\r\n    getItem,item\r\n  }\r\n}\r\n\r\nexport default {\r\n  name: \"ShopView\",\r\n  components: { ShopInfo, Content, Cart },\r\n  setup() {\r\n    const { getItem,item } = useGetItemEffect()\r\n    const { handleClickBack } = useRouterEffect()\r\n\r\n    getItem()\r\n    return {\r\n      item, getItem, handleClickBack\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n  padding: 0 0.18rem;\r\n}\r\n.search {\r\n  display: flex;\r\n  margin: 0.2rem 0 0.1rem 0;\r\n  line-height: 0.32rem;\r\n  &__icon {\r\n    margin-right: 0.11rem;\r\n    font-size: 0.32rem;\r\n    color: #b6b6b6;\r\n  }\r\n  &__content {\r\n    display: flex;\r\n    flex: 1;\r\n    padding: 0 0.16rem;\r\n    background: $search-bgColor;\r\n    border-radius: 16px;\r\n    input {\r\n      display: block;\r\n      border: none;\r\n      background: none;\r\n      outline: none;\r\n      height: 0.32rem;\r\n      width: 100%;\r\n      ::placeholder {\r\n        font-size: .14rem;\r\n        color: $content-fontcolor;\r\n        line-height: .16rem;\r\n      }\r\n    }\r\n    span {\r\n      font-size: 0.21rem;\r\n      color: $search-fontColor;\r\n      margin-right: 0.1rem;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}