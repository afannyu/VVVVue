{"ast":null,"code":"import { reactive } from '@vue/reactivity';\nimport { useRouter } from 'vue-router';\nimport { post } from '@/utils/request';\nimport store from '@/store';\nexport default {\n  name: 'RegisterView',\n\n  setup() {\n    const router = useRouter();\n    const data = reactive({\n      username: '',\n      password: '',\n      password2: ''\n    });\n\n    const handleRegister = async () => {\n      const result = await post('/api/user/register', {\n        username: data.username,\n        password: data.password,\n        password2: data.password2\n      });\n\n      if (result?.data === 0) {\n        localStorage.isLogin = true;\n        store.push({\n          name: 'Home'\n        });\n      }\n    };\n\n    const handleHave = () => {\n      router.push({\n        name: 'Login'\n      });\n    };\n\n    return {\n      handleHave,\n      data,\n      handleRegister\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,SAASA,QAAT,QAAyB,iBAAzB;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAASC,IAAT,QAAqB,iBAArB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,EAAxB;IACA,MAAMM,IAAG,GAAIP,QAAQ,CAAC;MACpBQ,QAAQ,EAAE,EADU;MAEpBC,QAAQ,EAAE,EAFU;MAGpBC,SAAS,EAAE;IAHS,CAAD,CAArB;;IAKA,MAAMC,cAAa,GAAI,YAAS;MAC9B,MAAMC,MAAK,GAAI,MAAMV,IAAI,CAAC,oBAAD,EAAsB;QAC7CM,QAAQ,EAAED,IAAI,CAACC,QAD8B;QAE7CC,QAAQ,EAAEF,IAAI,CAACE,QAF8B;QAG7CC,SAAS,EAAEH,IAAI,CAACG;MAH6B,CAAtB,CAAzB;;MAKA,IAAGE,MAAM,EAAEL,IAAR,KAAiB,CAApB,EAAuB;QACrBM,YAAY,CAACC,OAAb,GAAuB,IAAvB;QACAX,KAAK,CAACY,IAAN,CAAW;UAACX,IAAI,EAAE;QAAP,CAAX;MACF;IACF,CAVA;;IAWA,MAAMY,UAAS,GAAI,MAAI;MACrBV,MAAM,CAACS,IAAP,CAAY;QAACX,IAAI,EAAE;MAAP,CAAZ;IACF,CAFA;;IAGA,OAAO;MACLY,UADK;MAELT,IAFK;MAGLI;IAHK,CAAP;EAKF;;AA5Ba,CAAf","names":["reactive","useRouter","post","store","name","setup","router","data","username","password","password2","handleRegister","result","localStorage","isLogin","push","handleHave"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\register\\Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <img src=\"http://www.dell-lee.com/imgs/vue3/user.png\" alt=\"\">\r\n    <div class=\"wrapper__input\">\r\n      <input\r\n      v-model=\"data.username\"\r\n      type=\"text\"\r\n      placeholder=\"请输入用户名\"\r\n      >\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input\r\n      v-model=\"data.password\"\r\n      type=\"password\"\r\n      placeholder=\"请输入密码\"\r\n      >\r\n    </div>\r\n    <div class=\"wrapper__input\">\r\n      <input\r\n      v-model=\"data.password2\"\r\n      type=\"password\"\r\n      placeholder=\"确认密码\">\r\n    </div>\r\n    <div\r\n    @click=\"handleRegister\"\r\n    class=\"wrapper__register-button\"\r\n    >注册</div>\r\n    <div class=\"wrapper__register-link\" @click=\"handleHave\">已有账号去登录</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from '@vue/reactivity'\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '@/utils/request'\r\nimport store from '@/store'\r\n\r\nexport default {\r\n  name: 'RegisterView',\r\n  setup() {\r\n    const router = useRouter()\r\n    const data = reactive({\r\n      username: '',\r\n      password: '',\r\n      password2: ''\r\n    })\r\n    const handleRegister = async()=>{\r\n      const result = await post('/api/user/register',{\r\n        username: data.username,\r\n        password: data.password,\r\n        password2: data.password2\r\n      })\r\n      if(result?.data === 0) {\r\n        localStorage.isLogin = true\r\n        store.push({name: 'Home'})\r\n      }\r\n    }\r\n    const handleHave = ()=>{\r\n      router.push({name: 'Login'})\r\n    }\r\n    return {\r\n      handleHave,\r\n      data,\r\n      handleRegister\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n\r\n.wrapper {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY