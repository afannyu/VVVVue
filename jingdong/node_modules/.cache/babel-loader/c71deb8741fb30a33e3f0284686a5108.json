{"ast":null,"code":"import { reactive, toRefs } from '@vue/reactivity';\nimport { get } from '@/utils/request';\nexport default {\n  name: 'ContentView',\n\n  setup() {\n    const categoryList = [{\n      name: '全部商品',\n      tab: 'all'\n    }, {\n      name: '秒杀',\n      tab: 'seckill'\n    }, {\n      name: '新鲜水果',\n      tab: 'fruit'\n    }];\n\n    const handleCataClick = () => {\n      console.log(item.tab);\n    };\n\n    const data = reactive({\n      productList: []\n    });\n\n    const getProductData = async tab => {\n      const result = await get('/api/shop/1/products', {\n        tab\n      });\n\n      if (result?.errno === 0 && result?.data?.length) {\n        data.productList = result.data;\n      }\n    };\n\n    getProductData('all');\n    const {\n      productList\n    } = toRefs(data);\n    return {\n      productList,\n      getProductData,\n      categoryList,\n      handleCataClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,SAASA,QAAT,EAAmBC,MAAnB,QAAiC,iBAAjC;AACA,SAASC,GAAT,QAAoB,iBAApB;AACA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,YAAW,GAAI,CACnB;MACEF,IAAI,EAAE,MADR;MAEEG,GAAG,EAAE;IAFP,CADmB,EAKnB;MACEH,IAAI,EAAE,IADR;MAEEG,GAAG,EAAE;IAFP,CALmB,EASnB;MACEH,IAAI,EAAE,MADR;MAEEG,GAAG,EAAE;IAFP,CATmB,CAArB;;IAcA,MAAMC,eAAc,GAAI,MAAI;MAC1BC,OAAO,CAACC,GAAR,CAAYC,IAAI,CAACJ,GAAjB;IACF,CAFA;;IAGA,MAAMK,IAAG,GAAIX,QAAQ,CAAC;MAACY,WAAW,EAAE;IAAd,CAAD,CAArB;;IACA,MAAMC,cAAa,GAAI,MAAMP,GAAN,IAAY;MACjC,MAAMQ,MAAK,GAAI,MAAMZ,GAAG,CAAC,sBAAD,EAAwB;QAACI;MAAD,CAAxB,CAAxB;;MACA,IAAGQ,MAAM,EAAEC,KAAR,KAAkB,CAAlB,IAAuBD,MAAM,EAAEH,IAAR,EAAcK,MAAxC,EAAgD;QAC9CL,IAAI,CAACC,WAAL,GAAmBE,MAAM,CAACH,IAA1B;MACF;IACF,CALA;;IAMAE,cAAc,CAAC,KAAD,CAAd;IACA,MAAM;MAAED;IAAF,IAAkBX,MAAM,CAACU,IAAD,CAA9B;IACA,OAAO;MACLC,WADK;MACQC,cADR;MAELR,YAFK;MAESE;IAFT,CAAP;EAKF;;AAlCa,CAAf","names":["reactive","toRefs","get","name","setup","categoryList","tab","handleCataClick","console","log","item","data","productList","getProductData","result","errno","length"],"sourceRoot":"","sources":["C:\\Carrie\\VVVVVue\\jingdong\\src\\views\\shop\\Content.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"category\">\r\n      <div\r\n      v-for=\"(item,index) in categoryList\"\r\n      :key=\"index\"\r\n      class=\"category__item\"\r\n      @click=\"()=>handleCataClick(item.tab)\"\r\n      >\r\n      {{item.name}}\r\n      </div>\r\n    </div>\r\n    <div class=\"product\">\r\n      <div\r\n      v-for=\"item in productList\"\r\n      :key=\"item._id\"\r\n      class=\"product__item\"\r\n      >\r\n        <img :src=\"item.imgUrl\" alt=\"\">\r\n        <div class=\"product__item__text\">\r\n          <h4>{{item.name}}</h4>\r\n          <p>月售{{item.sales}}件</p>\r\n          <div class=\"price\">\r\n            <span class=\"yen\">￥{{item.price}}</span>\r\n            <span class=\"origin\">￥{{item.oldPrice}}</span>\r\n          </div>\r\n          <div class=\"number\">\r\n            <div class=\"number__minus\">-</div>\r\n            <div class=\"number__num\">0</div>\r\n            <div class=\"number__plus\">+</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from '@vue/reactivity'\r\nimport { get } from '@/utils/request'\r\nexport default {\r\n  name: 'ContentView',\r\n  setup() {\r\n    const categoryList = [\r\n      {\r\n        name: '全部商品',\r\n        tab: 'all'\r\n      },\r\n      {\r\n        name: '秒杀',\r\n        tab: 'seckill'\r\n      },\r\n      {\r\n        name: '新鲜水果',\r\n        tab: 'fruit'\r\n      }\r\n    ]\r\n    const handleCataClick = ()=>{\r\n      console.log(item.tab)\r\n    }\r\n    const data = reactive({productList: []})\r\n    const getProductData = async(tab)=>{\r\n      const result = await get('/api/shop/1/products',{tab})\r\n      if(result?.errno === 0 && result?.data?.length) {\r\n        data.productList = result.data\r\n      }\r\n    }\r\n    getProductData('all')\r\n    const { productList } = toRefs(data)\r\n    return {\r\n      productList, getProductData,\r\n      categoryList, handleCataClick\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n\r\n.content {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 1.68rem;\r\n  bottom: .49rem;\r\n}\r\n\r\n.category {\r\n  height: 100%;\r\n  width: .76rem;\r\n  overflow-y: scroll;\r\n  background: $search-bgColor;\r\n  border-radius: 2px;\r\n  &__item {\r\n    line-height: .4rem;\r\n    text-align: center;\r\n    font-size: .14rem;\r\n    color: $content-fontcolor;\r\n    &--active {\r\n      background: $bgColor;\r\n    }\r\n  }\r\n}\r\n\r\n.product {\r\n  flex: 1;\r\n  margin: 0 .18rem 0 .16rem;\r\n  overflow-y: scroll;\r\n  &__item {\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    border-bottom: .01rem solid $content-bgcolor;\r\n    color: $content-fontcolor;\r\n    img {\r\n      margin-right: .16rem;\r\n      height: .68rem;\r\n      width: .68rem;\r\n    }\r\n    &__text {\r\n      position: relative;\r\n      flex: 1;\r\n      overflow: hidden;\r\n      h4 {\r\n        margin: 0;\r\n        font-size: .14rem;\r\n        line-height: .2rem;\r\n        color: $content-fontcolor;\r\n        @include ellipsis\r\n      }\r\n      p {\r\n        margin: .06rem 0;\r\n        line-height: .16rem;\r\n        font-size: .12rem;\r\n        color: $content-fontcolor;\r\n      }\r\n      .price {\r\n        .yen {\r\n          margin-right: .02rem;\r\n          line-height: .2rem;\r\n          font-size: .12rem;\r\n          color: $highlight-fontColor;\r\n        }\r\n        .origin {\r\n          display: inline-block;\r\n          font-size: .12rem;\r\n          transform: scale(.83);\r\n          transform-origin: center;\r\n          text-decoration: line-through;\r\n          color: $light-fontColor;\r\n        }\r\n      }\r\n      .number {\r\n        position: absolute;\r\n        display: flex;\r\n        line-height: .2rem;\r\n        right: 0;\r\n        bottom: 0;\r\n        &__num {\r\n          margin: 0 .1rem;\r\n          font-size: .14rem;\r\n        }\r\n        &__minus,&__plus {\r\n          line-height: .16rem;\r\n          box-sizing: border-box;\r\n          height: .2rem;\r\n          width: .2rem;\r\n          font-size: .18rem;\r\n          text-align: center;\r\n          border-radius: 50%;\r\n        }\r\n        &__minus {\r\n          border: .01rem solid $mid-fontColor;\r\n          color: $mid-fontColor;\r\n        }\r\n        &__plus {\r\n          background: $btn-bgColor;\r\n          color: $bgColor;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}