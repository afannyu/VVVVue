{"ast":null,"code":"import ShopInfo from \"@/components/ShopInfo\";\nimport { useRoute, useRouter } from 'vue-router';\nimport { reactive, toRefs } from '@vue/reactivity';\nimport { get } from '@/utils/request';\n\nconst useGetItemEffect = () => {\n  const route = useRoute();\n  const data = reactive({\n    item: {}\n  });\n\n  const getItem = async () => {\n    const result = await get(`/api/shop/${route.params.id}`);\n\n    if (result?.errno === 0 && result?.data) {\n      data.item = result.data[`${route.params.id - 1}`];\n    }\n\n    console.log(result);\n  };\n\n  const {\n    item\n  } = toRefs(data);\n  return {\n    getItem,\n    item\n  };\n};\n\nconst useRouterBack = () => {\n  const router = useRouter();\n\n  const handleBack = () => {\n    router.back();\n  };\n\n  return {\n    handleBack\n  };\n};\n\nexport default {\n  name: \"ShopView\",\n  components: {\n    ShopInfo\n  },\n\n  setup() {\n    const {\n      getItem,\n      item\n    } = useGetItemEffect();\n    const {\n      handleBack\n    } = useRouterBack();\n    getItem();\n    return {\n      item,\n      handleBack,\n      getItem\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,QAAP,MAAqB,uBAArB;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAoC,YAApC;AACA,SAASC,QAAT,EAAmBC,MAAnB,QAAiC,iBAAjC;AACA,SAASC,GAAT,QAAoB,iBAApB;;AAEA,MAAMC,gBAAe,GAAI,MAAI;EAC3B,MAAMC,KAAI,GAAIN,QAAQ,EAAtB;EACA,MAAMO,IAAG,GAAIL,QAAQ,CAAC;IAACM,IAAI,EAAE;EAAP,CAAD,CAArB;;EACA,MAAMC,OAAM,GAAI,YAAU;IACxB,MAAMC,MAAK,GAAI,MAAMN,GAAG,CAAE,aAAYE,KAAK,CAACK,MAAN,CAAaC,EAAG,EAA9B,CAAxB;;IACA,IAAGF,MAAM,EAAEG,KAAR,KAAkB,CAAlB,IAAuBH,MAAM,EAAEH,IAAlC,EAAuC;MACrCA,IAAI,CAACC,IAAL,GAAYE,MAAM,CAACH,IAAP,CAAa,GAAED,KAAK,CAACK,MAAN,CAAaC,EAAb,GAAkB,CAAE,EAAnC,CAAZ;IACF;;IACAE,OAAO,CAACC,GAAR,CAAYL,MAAZ;EACF,CANA;;EAOA,MAAM;IAAEF;EAAF,IAAWL,MAAM,CAACI,IAAD,CAAvB;EACA,OAAO;IACLE,OADK;IACGD;EADH,CAAP;AAGF,CAdA;;AAgBA,MAAMQ,aAAY,GAAI,MAAI;EACxB,MAAMC,MAAK,GAAIhB,SAAS,EAAxB;;EACA,MAAMiB,UAAS,GAAI,MAAI;IACrBD,MAAM,CAACE,IAAP;EACF,CAFA;;EA